        "2": {
          "title": "Diferença O.O e Procedural",
          "id": "10.56a6.db03370d3-564d.29a4b1e0a",
          "ideas": {
            "1": {
              "title": "Em O.O os atributos e comportamentos estão em um único objeto.",
              "id": "11.56a6.db03370d3-564d.29a4b1e0a"
            },
            "2": {
              "title": "Procedural os atributos e comportamentos estão normalmente separados",
              "id": "12.56a6.db03370d3-564d.29a4b1e0a"
            }
          },
          "attr": {
            "position": [
